<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/css" href="Schema4Css.css"?>
<document xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Schema4.xsd">
	<dimensions>
		<dimension>
			<name>Release</name>
			<tags>
				<tag>Full</tag>
				<tag>Trial</tag>
			</tags>
			<default>Trial</default>
		</dimension>
		<dimension>
			<name>Java Version</name>
			<tags>
				<tag>1.4</tag>
				<tag>5</tag>
			</tags>
			<default>1.4</default>
		</dimension>
	</dimensions>

	<part>
		<data>class GoodApp{</data>
		<subObjects>
			<choice>
				<alternatives>
					<alternative>
						<part>
							<data>List &lt;Job&gt;</data>
						</part>
						<label>
							<tag>5.0</tag>
						</label>
					</alternative>
					<alternative>
						<part>
							<data>List</data>
						</part>
						<label>
							<tag>1.4</tag>
						</label>
					</alternative>
				</alternatives>
			</choice>
			<part>
				<data>jobs =</data>
				<subObjects>
					<choice>
						<alternatives>
							<alternative>
								<part>
									<data>new LinkedList&lt;Job&gt;()</data>
								</part>
								<label>
									<tag>5.0</tag>
								</label>
							</alternative>
							<alternative>
								<part>
									<data>new LinkedList();</data>
								</part>
								<label>
									<tag>1.4</tag>
								</label>
							</alternative>
						</alternatives>
					</choice>
					<part>
						<data>void runJobs(){</data>
						<subObjects>
							<choice>
								<!-- trialCount^T, NULL^F -->
								<alternatives>
									<alternative>
										<part>
											<data>int trialCount=0;</data>
										</part>
										<label>
											<tag>Trial</tag>
										</label>
									</alternative>
									<alternative>
										<part>
											<data/>
										</part>
										<label>
											<tag>Full</tag>
										</label>
									</alternative>
								</alternatives>
							</choice>
							<let>
								<binding>
									<name>r</name>
									<value>
										<part>
											<data>j.run</data>
										</part>
									</value>
								</binding>
								<body>
									<let>
										<binding>
											<name>i</name>
											<value>
												<choice>
													<alternatives>
														<alternative>
															<part>
																<data>if (++trialCount > 5) break;</data>
															</part>
															<label>
																<tag>Trial</tag>
															</label>
														</alternative>
														<alternative>
															<part>
																<data/>
															</part>
															<label>
																<tag>Full</tag>
															</label>
														</alternative>
													</alternatives>
												</choice>
											</value>
										</binding>
										<body>
											<choice>
												<alternatives>
													<alternative>
														<part>
															<data/>
															<subObjects>
																<part>
																	<data>for (Job j: jobs)</data>
																	<subObjects>
																		<part><data>{</data></part>
																		<part>
																			<data/>
																			<subObjects>
																				<variable>r</variable>
																				<variable>i</variable>
																			</subObjects>
																		</part>
																		<part><data>}</data></part>
																	</subObjects>
																</part>
															</subObjects>
														</part>
														<label>
															<tag>5.0</tag>
														</label>
													</alternative>
													<alternative>
														<part>
															<data/>
															<subObjects>
																<part><data>Iterator it = jobs.iterator();</data></part>
																<part>
																	<data>while(it.hasNext())</data>
																	<subObjects>
																		<part>
																			<data>{</data>
																		</part>
																		<part>
																			<data/>
																			<subObjects>
																				<part><data>j = (Job) it.next();</data></part>
																				<variable>r</variable>
																				<variable>i</variable>
																			</subObjects>
																		</part>
																		<part>
																			<data>}</data>
																		</part>
																	</subObjects>
																</part>
															</subObjects>
														</part>
														<label>
															<tag>1.4</tag>
														</label>
													</alternative>
												</alternatives>
											</choice>
										</body>
									</let>
								</body>
							</let>
						</subObjects>
					</part>
				</subObjects>
			</part>
		</subObjects>
	</part>

</document>
